generator client {
  provider = "prisma-client-js"
  output   = "../src/lib/prisma/generated"
}

datasource db {
  provider = "sqlite"
  url      = env("DB_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String  @unique
  password  String
  email     String?
  createdAt DateTime @default(now())
}
